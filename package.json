      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "9",
    "cross-env": "^7.0.3",
    "postcss": "7",
    "postcss-cli": "7.1.2",
    "tailwindcss": "^2.0.1-compat"
  }
