  "dependencies": {
    "react": "^17.0.1",
    "react-confetti": "^6.0.0",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "react-spring": "^8.0.27",
    "react-toastify": "^6.1.0",
    "react-with-gesture": "^4.0.8",
    "statedrive-react": "^0.0.8",
